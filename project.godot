; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="WiremockGUI"
run/main_scene="res://scenes/screens/server_list_screen.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

WiremockClient="*res://scripts/api/wiremock/wiremock_client.gd"
SceneManager="*res://scripts/states/scene_manager.gd"
ToastManager="*res://scripts/states/toast_manager.gd"
ServerListClient="*res://scripts/api/server_list/server_list_client.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"

[gui]

theme/custom="res://assets/themes/theme.tres"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
